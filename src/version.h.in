#ifndef VERSION_H
#define VERSION_H

#include <string>

inline std::string git_tag() {
    return "@GIT_TAG@";
}

inline std::string git_hash() {
    return "@GIT_HASH@";
}

inline std::string compile_date() {
    return "@COMPILE_DATE@";
}

inline std::string release_mode() {
    return "@CMAKE_BUILD_TYPE@";
}

#endif // VERSION_H
